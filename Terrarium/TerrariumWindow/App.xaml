<Application x:Class="TerrariumWindow.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TerrariumWindow"
             StartupUri="PopupWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="LayoutStyles/ForestStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="LayoutStyles/JungleStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="LayoutStyles/OceanStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="LayoutStyles/PokemonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="LayoutStyles/RoboticStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>